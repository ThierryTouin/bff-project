{
  "realm": "myrealm",
  "enabled": true,
  "clients": [
    {
      "clientId": "bff-client",
      "secret": "secret",
      "enabled": true,
      "protocol": "openid-connect",
      "redirectUris": ["http://localhost:4200/*", "http://localhost:8081/*"],
      "publicClient": false,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "defaultClientScopes": [
        "web-origins",
        "profile",
        "email",
        "roles"
      ]
